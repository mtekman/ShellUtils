#
#  Git update scripts
#

# Sets temp varaiables that only exist for the same bash session

alias gpu="git push -u"
alias gpull="git pull origin master && fpull=\"Y\""
function gcomm() { [ "$fpull" = "Y" ] && git commit -a -m $* && fcomm="Y" || echo "Have you pulled first?"; }
function gpuom() { [ "$fcomm" = "Y" ] && gpu origin master || echo "Have you committed first?"; }
